{
  "extends": ["eslint:recommended"],
  "plugins": ["prettier", "import"],
  "rules": {
    "prettier/prettier": "error"
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": false,
      "generators": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  }
}
